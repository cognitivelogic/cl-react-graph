    "d3-selection-multi": "^1.0.1",
    "deepmerge": "^1.3.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "ts-loader": "^3.1.1",
    "tslint-eslint-rules": "^4.1.1"
  }
