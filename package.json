  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cognitivelogic/cl-react-graph.git"
  },
  "keywords": [
    "React",
    "D3",
    "Charts"
  ],
  "author": "Rob Clayburn - Infosum",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cognitivelogic/cl-react-graph/issues"
  },
  "homepage": "https://github.com/cognitivelogic/cl-react-graph#readme",
  "devDependencies": {
    "@types/d3": "^4.12.0",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.12.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  },
  "babel": {
    "plugins": [
      "transform-flow-strip-types"
    ],
    "presets": [
      "env",
      "stage-0",
      "react"
    ]
