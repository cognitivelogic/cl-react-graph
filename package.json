{
  "name": "cl-react-graph",
  "version": "1.1.0",
  "description": "React D3 Charts",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "build-examples": "webpack --config ./webpack.config.examples.js",
