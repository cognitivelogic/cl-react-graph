{
  "name": "cl-react-graph",
  "version": "1.0.1",
  "description": "React D3 Charts",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "build-examples": "webpack --config ./webpack.config.examples.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "istanbul cover _mocha --recursive -x '**/*.spec.js' -x '**/config/**' -x '**/node_modules/**' -g './{,!(node_modules)/**/}*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cognitivelogic/cl-react-graph.git"
  },
  "keywords": [
    "React",
    "D3",
    "Charts"
  ],
  "author": "Rob Clayburn - Cognitive Logic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cognitivelogic/cl-react-graph/issues"
  },
  "homepage": "https://github.com/cognitivelogic/cl-react-graph#readme",
  "devDependencies": {
    "@types/d3": "^4.9.0",
    "@types/react": "^15.0.27",
    "@types/react-dom": "^15.5.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "flow-bin": "^0.44.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "webpack": "^2.3.3"
  },
  "babel": {
    "plugins": [
      "transform-flow-strip-types"
    ],
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "dependencies": {
    "d3": "^4.7.4",
    "d3-selection-multi": "^1.0.1",
    "deepmerge": "^1.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "ts-loader": "^2.1.0",
    "tslint-eslint-rules": "^4.1.1"
  }
}
